{
  "name": "YAWE (Yet Another Wiki Extension)",
  "version": "2.0.3",
  "description": "Read and Search for Wikipedia Articles",
  "icons": {
      "16" : "images/icon.png",
 			"48" : "images/48.png",
			"128" : "images/128.png"
  },
	"permissions": [
	  "*://*.wikipedia.org/"
	],
  "options_page": "options.html",
	"browser_action": {
    "default_icon": "images/icon.png", 
    "default_title": "YAWE (Yet Another Wiki Extension)",      
    "default_popup": "search.html"       
  },
  "manifest_version": 2
}