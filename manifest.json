{
  "name": "YAWE (Yet Another Wiki Extension)",
  "version": "3.1.3",
  "description": "Read and Search for Wikipedia Articles",
  "icons": {
    "16" : "images/icon.png",
    "48" : "images/48.png",
    "128" : "images/128.png"
  },
  "sandbox": {
    "pages": [
      "index.html"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/icon.png", 
    "default_title": "YAWE (Yet Another Wiki Extension)",      
    "default_popup": "popup.html"       
  },
  "manifest_version": 2,
  "permissions": ["storage"]
}